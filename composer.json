{
    "name": "Congresso Gestalt Rio",
    "description": "CMS bases on CodeIgniter.",
    "keywords": ["framework", "codeigniter", "cms"],
    "license": "MIT",
    "require": {
	    "ensepar/html2pdf": "1.0.*@dev",
	    "illuminate/database": "4.*",
	    "patchwork/utf8": "1.1.*"
    },
    "require-dev": {

    },
    "autoload": {
        "classmap": [
            "vendor/"
        ],
	    "psr-4": {
		    "Gestalt\\": "app/Gestalt",
		    "Src\\": "src/",
		    "Cms\\": "app/modules/cms/Cms"
	    }
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 4000,
        "vendor-dir": "vendor"
    },
    "minimum-stability": "stable"
}